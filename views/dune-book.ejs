<div class="row row-cols-1 row-cols-md-3 gx-4 gy-4 text-center">
    <% for (let i=0; i<data.length; i++) {%>
        <div class="col themed-grid-col">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title"><%= data[i].title %></h5>
                    <h6 class="card-subtitle mb-2 text-body-secondary">Author: <%= data[i].author %> Year: <%= data[i].year %></h6>
                    <a href="<%= data[i].wiki_url %>" class="card-link">Wiki</a>
                </div>
            </div>
        </div>
    <%}%>
</div>